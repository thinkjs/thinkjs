{
  "name": "thinkjs",
  "description": "ThinkJS - use full ES6/7 features to develop web applications",
  "version": "2.0.11",
  "author": {
    "name": "welefen",
    "email": "welefen@gmail.com"
  },
  "scripts": {
    "test": "mocha --reporter spec --timeout 2000 --recursive test/",
    "test-cov": "istanbul cover ./node_modules/mocha/bin/_mocha -- -t 2000 --recursive  -R spec test/",
    "compile": "babel --loose all --optional runtime --stage 0 --modules common src/ --out-dir lib/",
    "watch-compile": "npm run compile -- --watch",
    "watch": "npm run watch-compile",
    "prepublish": "npm run compile",
    "eslint": "eslint src/",
    "test-all": "npm run eslint; npm run test-cov"
  },
  "bin": {
    "thinkjs": "./bin/index.js"
  },
  "contributors": [{
    "name": "welefen",
    "email": "welefen@gmail.com"
  }],
  "main": "lib/index.js",
  "dependencies": {
    "ejs": "2.3.4",
    "multiparty": "4.1.2",
    "mime": "1.3.4",
    "mysql": "2.9.0",
    "thinkit": "3.3.0",
    "babel-runtime": "5.8.25",
    "babel-core": "5.8.33",
    "co": "4.6.0",
    "colors": "1.1.2",
    "validator": "4.2.0",
    "commander": "2.9.0"
  },
  "devDependencies": {
    "mocha": "1.20.1",
    "muk": "0.3.1",
    "istanbul": "0.4.0",
    "babel": "5.8.23",
    "eslint": "1.10.2"
  },
  "keywords": [
    "thinkjs",
    "framework",
    "web",
    "rest",
    "restful",
    "router",
    "api",
    "es6",
    "es7",
    "async",
    "await",
    "yield",
    "websocket",
    "generator-function"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/welefen/thinkjs"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/welefen/thinkjs/issues"
  }
}
